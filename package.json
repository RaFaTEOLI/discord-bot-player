{
  "name": "discord-bot-player",
  "version": "0.0.0",
  "description": "Discord Bot service to play songs",
  "main": "src/main/server.ts",
  "repository": "git@github.com:RaFaTEOLI/discord-bot-player.git",
  "author": "RaFaTEOLI",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "node ./dist/src/main/server.js",
    "dev": "tsnd --respawn --transpile-only ./src/main/server.ts",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --collect-coverage"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "discord-music-player": "^9.1.1",
    "dotenv": "^16.0.3",
    "isomorphic-unfetch": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "discord.js": "^14.5.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
